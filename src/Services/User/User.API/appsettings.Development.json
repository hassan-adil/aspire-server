{
  "Service": {
    "Name": "User.API",
    "Version": "1.0.0-dev"
  },
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "Keycloak": {
    "ServerUrl": "http://localhost:8081",
    "Realm": "Aspire"
  },
  "ServiceRegistry": {
    "Services": [
      {
        "Name": "Auth",
        "BaseUrl": "http://localhost:5210",
        "TimeoutSeconds": 5,
        "Endpoints": {
          "GetAdminToken": {
            "Path": "WeatherForecast/TestAdminToken",
            "Method": "GET"
          }
        }
      }
    ]
  }
}
